{
  "name": "webpage-portfolio-angular",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "ng:start": "ng serve",
    "ng:build": "ng build angular-version --prod --base-href=/angular-version/",
    "ng:test": "ng test",
    "start": "run-s clean copy:assets copy:images copy:categories ng:start",
    "build": "run-s clean copy:assets copy:images copy:categories ng:build copy:configs",
    "test": "run-s clean copy:assets copy:images copy:categories ng:test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf src/assets/base/**/*.*",
    "copy:assets": "cpx ../webpage-portfolio/assets/*.* src/assets/base",
    "copy:categories": "cpx ../webpage-portfolio/js/categories.js src/assets/base",
    "copy:images": "cpx ../webpage-portfolio/images/*.* src/assets/base",
    "copy:configs": "cpx src/assets/.htaccess dist/angular-version/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.13",
    "@angular/common": "~8.2.13",
    "@angular/compiler": "~8.2.13",
    "@angular/core": "~8.2.13",
    "@angular/forms": "~8.2.13",
    "@angular/platform-browser": "~8.2.13",
    "@angular/platform-browser-dynamic": "~8.2.13",
    "@angular/router": "~8.2.13",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "bootstrap": "^4.4.1",
    "font-awesome": "^4.7.0",
    "install": "^0.13.0",
    "npm": "^6.14.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.18",
    "@angular/cli": "~8.3.18",
    "@angular/compiler-cli": "~8.2.13",
    "@angular/language-service": "~8.2.13",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
